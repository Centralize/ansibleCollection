zone "{{ bind_test_zone_name }}" {
    type master;
    file "/etc/bind/zones/db.{{ bind_test_zone_name }}";
};

zone "{{ bind_test_zone_ip_prefix.split('.') | reverse | join('.') }}.in-addr.arpa" {
    type master;
    file "/etc/bind/zones/db.{{ bind_test_zone_ip_prefix.split('.') | reverse | join('.') }}.in-addr.arpa";
};
